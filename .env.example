# Availity API Credentials
# Copy this file to .env and replace with your actual credentials
# Note: Vite requires environment variables to be prefixed with VITE_
VITE_AVAILITY_CLIENT_ID=your_client_id_here
VITE_AVAILITY_CLIENT_SECRET=your_client_secret_here
VITE_AVAILITY_API_TOKEN_URL=https://api.availity.com/availity/v1/token
VITE_AVAILITY_API_BASE_URL=https://api.availity.com/availity/development-partner/v1

# Backend Server Configuration
VITE_BACKEND_URL=http://localhost:3001
SERVER_PORT=3001
